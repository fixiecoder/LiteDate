{
  "name": "utc-date",
  "version": "0.0.1",
  "description": "Simple lightweight UTC date object tool that allows date formatting and manipulation",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc mocha --reporter landing",
    "build": "export mode=development && webpack --watch",
