  },
  "browser": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fixiecoder/utc-date.git"
  },
  "keywords": [
    "date",
    "moment",
    "alternative",
    "lite",
    "simple",
    "easy",
    "time",
    "datetime"
  ],
  "author": "fixiecoder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fixiecoder/utc-date/issues"
  },
  "homepage": "https://github.com/fixiecoder/utc-date#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "mocha": "^5.0.3",
    "nyc": "^11.4.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  }
